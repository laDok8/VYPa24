class Car {
    String color;
    int year;

    void start() {
        System.out.println("Car started!");
    }
}

public class Main {
    public static void main(String[] args) {
        // Create a car object
        Car myCar = new Car();
        myCar.color = "red";
        myCar.year = 2023;
        myCar.start();

        // Test if the car's color is red
        if (myCar.color.equals("red")) {
            System.out.println("Car is red!");
        } else {
            System.out.println("Car is not red.");
        }
    }
}