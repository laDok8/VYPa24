#! /bin/vypint
# VYPcode: 1.0
# Generated by xdokou14

ALIAS BP $0
ALIAS AX $1
ALIAS BX $2
ALIAS CX $3
ALIAS DX $4
ALIAS EX $5
ALIAS SI $6
ALIAS DI $7
SET $BP $SP
CALL [$SP+1] main
JUMP _end

# VMT for Object
CREATE $AX 2
SETWORD $AX 0 "toString"
SETWORD $AX 1 "getClass"
ADDI $SP, $SP, 1
SET [$SP] $AX



LABEL main
	ADDI $SP, $SP, 1 # space for PC
	ADDI $SP, $SP, 1
	SET [$SP] $BP
	SET $BP $SP
	# declare firstPart
	SET [$BP+1] 0
	ADDI $SP, $SP, 1

	# restore stack
	ADDI $SP $BP 1

	# declare secondPart
	SET [$BP+2] 0
	ADDI $SP, $SP, 1

	# restore stack
	ADDI $SP $BP 2

	# declare result
	SET [$BP+3] 0
	ADDI $SP, $SP, 1

	# restore stack
	ADDI $SP $BP 3

	# string literal "Hello, "
	CREATE $DI 1
	SETWORD $DI 0 "Hello, "
	GETWORD $DI $DI 0
	ADDI $SP, $SP, 1
	SET [$SP] $DI

	# assign var firstPart
	SET [$BP+1] [$SP]

	# restore stack
	ADDI $SP $BP 3

	# string literal "World!"
	CREATE $DI 1
	SETWORD $DI 0 "World!"
	GETWORD $DI $DI 0
	ADDI $SP, $SP, 1
	SET [$SP] $DI

	# assign var secondPart
	SET [$BP+2] [$SP]

	# restore stack
	ADDI $SP $BP 3

	# push object ref firstPart
	ADDI $SP, $SP, 1
	SET [$SP] [$BP+1]

	# push object ref secondPart
	ADDI $SP, $SP, 1
	SET [$SP] [$BP+2]

	# binary operation: +
	ADDI $AX [$SP] [$SP-1]

	SUBI $SP, $SP, 1
	SET [$SP] $AX

	# assign var result
	SET [$BP+3] [$SP]

	# restore stack
	ADDI $SP $BP 3

	# string literal "Concatenated String: "
	CREATE $DI 1
	SETWORD $DI 0 "Concatenated String: "
	GETWORD $DI $DI 0
	ADDI $SP, $SP, 1
	SET [$SP] $DI

	# push object ref result
	ADDI $SP, $SP, 1
	SET [$SP] [$BP+3]

	# string literal "\n"
	CREATE $DI 1
	SETWORD $DI 0 "\n"
	GETWORD $DI $DI 0
	ADDI $SP, $SP, 1
	SET [$SP] $DI

	# print
	WRITES [$SP-2]
	WRITES [$SP-1]
	WRITES [$SP]
	# restore stack
	ADDI $SP $BP 3

	# exit function
	SET $SP $BP
	SET $BP [$SP]
	SUBI $SP, $SP, 1
	SUBI $SP, $SP, 1
	RETURN [$SP+1]

LABEL _end
