class Shape : Object {
    int id;
    void Shape ( void ) { print("Shape constructor\n"); }
    string toString(void) { return "Shape" + (string)this.id;}
}
class Rectangle : Shape {
    int width, height;
    string toString(void) { return super.toString() + " - Rectangle" + (string)this.width + "x" + (string)this.area(); }
    int area(void) { return this.width * this.height; }
}

