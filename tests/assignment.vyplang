void test2 ( int x, string z )
{
    int a, res;
    print("Enter an integer to compute its factorial:");
    a = readInt();
    if (a < 0) {
        print("\nFactorial of a negative integer is undefined!\n"); }
    else {
        res = 1;
    while (a > 0) {
        res = res * a; a = a - 1;
    } // endwhile
    print("\nThe result is: ", res, "\n");
    } // endif
}

// example of assignment of an integer value and its print out
void main ( void )
{
test2();
}  // end of main
